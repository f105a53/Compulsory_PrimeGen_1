<Window
    x:Class="GUI.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:GUI"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <DockPanel LastChildFill="True">
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
            <TextBox Name="lower" Width="150">1000000</TextBox>
            <TextBox Name="upper" Width="150">10000000</TextBox>
            <Button Click="SequentialButtonClick">Run Sequentially</Button>
            <Button Click="ParallelButtonClick">Run in Parallel</Button>
        </StackPanel>
        <ListBox Name="ListBox" />
    </DockPanel>
</Window>
